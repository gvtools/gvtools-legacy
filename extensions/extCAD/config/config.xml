<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<libraries library-dir="./"/>
	<depends plugin-name="com.iver.cit.gvsig" />
	<depends plugin-name="com.iver.cit.gvsig.jdbc_spatial" />
	<resourceBundle name="text"/>
	<extensions>
		<extension class-name="com.iver.cit.gvsig.CreateNewLayer"
			description="Crear una nueva capa."
			active="true" >
			<menu text="Vista/new_layer/new_shp" key="n" position="-90" action-command="SHP"/>
			<menu text="Vista/new_layer/new_postgis" icon="images/postgres.png" position="1" action-command="POSTGIS"/>
		</extension>

		<extension class-name="com.iver.cit.gvsig.ExportTo"
			description="Exportar a diversos formatos"
			active="true">
			<menu text="Capa/export_to/shp" action-command="SHP" position="-80"/>
			<menu text="Capa/export_to/postgis" icon="images/postgres.png" action-command="POSTGIS" position="3"/>
		</extension>

		<extension class-name="com.iver.cit.gvsig.StartEditing"
			description="Extensión encargada de comenzar la edición sobre la capa activa."
			active="true">
			<menu text="Capa/start_edition" key="e" action-command="STARTEDITING" position="3" icon="images/start.png"/>
		</extension>

		<extension class-name="com.iver.cit.gvsig.StopEditing"
			description="Extensión encargada de terminar la edición sobre la capa activa y guardarla sobre sí misma."
			active="true">
			<menu text="Capa/stop_edition" key="e" action-command="STOPEDITING" position="3" icon="images/stop.png"/>
		</extension>

		<!-- <extension class-name="com.iver.cit.gvsig.StopEditingToGT2Shp"
			description="Extensión encargada de terminar la edición sobre la capa activa y guardarla sobre sí misma si es un shapefile."
			active="true">
			<menu text="Capa/stop_edition/GT2shp" action-command="STOPEDITING"/>
		</extension> -->

		<extension class-name="com.iver.cit.gvsig.InsertPointExtension"
			description="Extensión encargada de insertar puntos en una capa en edición."
			active="true">
			<menu text="geometry/insert/point" action-command="_point" icon="edition-insert-geometry-point" position="-50"/>
			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-insert-geometry-point" action-command="_point" enable-text="point" tooltip="point" position="1"/>
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.InsertMultiPointExtension"
			description="Extensión encargada de insertar multipuntos en una capa en edición."
			active="true">
			<menu text="geometry/insert/multipoint" action-command="_multipoint" icon="edition-insert-geometry-multipoint"/>
			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-insert-geometry-multipoint" action-command="_multipoint" enable-text="multipoint" tooltip="multipoint" position="2"/>
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.InsertLineExtension"
			description="Extensión encargada de insertar líneas en una capa en edición."
			active="true">
			<menu text="geometry/insert/line" action-command="_line" icon="edition-geometry-insert-line"/>
			<menu text="geometry/insert/arc" action-command="_arc" icon="edition-insert-geometry-arc"/>

			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-geometry-insert-line" action-command="_line" enable-text="line" tooltip="line" position="3"/>
				<selectable-tool icon="edition-insert-geometry-arc" action-command="_arc" enable-text="arc" tooltip="arc" position="4"/>
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.InsertPolyLineExtension"
			description="Extensión encargada de insertar polilíneas en una capa en edición."
			active="true">
			<menu text="geometry/insert/polyline" action-command="_polyline" icon="edition-insert-geometry-polyline"/>
			<!--menu text="geometry/insert/spline" action-command="_spline" icon="edition-insert-geometry-spline"/-->

			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-insert-geometry-polyline" action-command="_polyline" enable-text="polyline" tooltip="polyline" position="5"/>
				<!--selectable-tool icon="edition-insert-geometry-spline" action-command="_spline" tooltip="spline" position="5"/-->
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.InsertPolygonExtension"
			description="Extensión encargada de insertar poligonos en una capa en edición."
			active="true">
			<menu text="geometry/insert/polygon" action-command="_polygon" icon="edition-insert-geometry-polygon"/>
			<menu text="geometry/insert/rectangle" action-command="_rectangle" icon="edition-insert-geometry-rectangle"/>
			<menu text="geometry/insert/circle" action-command="_circle" icon="edition-insert-geometry-circle"/>
			<menu text="geometry/insert/ellipse" action-command="_ellipse" icon="edition-insert-geometry-ellipse"/>
			<menu text="geometry/modify/edit_vertex" key="v" action-command="_editvertex" icon="edition-geometry-edit-vertex"/>
			<menu text="geometry/modify/edit_vertex" is_separator="true"/>
			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-insert-geometry-polygon" action-command="_polygon" enable-text="polygon" tooltip="polygon" position="6"/>
				<selectable-tool icon="edition-insert-geometry-rectangle" action-command="_rectangle" enable-text="rectangle" tooltip="rectangle" position="7"/>
				<selectable-tool icon="edition-insert-geometry-circle" action-command="_circle" enable-text="circle" tooltip="circle" position="8"/>
				<selectable-tool icon="edition-insert-geometry-ellipse" action-command="_ellipse" enable-text="ellipse" tooltip="ellipse" position="9"/>
			</tool-bar>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-edit-vertex" action-command="_editvertex" enable-text="edit_vertex" tooltip="edit_vertex" position="7"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.AutoCompletePolygonExtension"
			description="Extensión encargada de restablecer la selección anterior."
			active="true">
			<menu text="geometry/insert/autopolygon" action-command="AUTOCOMPLETE_POLYGON" icon="edition-geometry-autocompletepolygon"/>
			<tool-bar name="cad_insert" position="30">
				<selectable-tool icon="edition-geometry-autocompletepolygon" action-command="AUTOCOMPLETE_POLYGON" enable-text="autocomplete-polygon" tooltip="autocomplete-polygon" position="20"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.CADExtension"
			description="Extensión encargada de gestionar los eventos realizados sobre las geometrías."
			active="true">
			<menu text="geometry/modify/copy" action-command="_copy" icon="edition-geometry-copy" position="1"/>
			<menu text="geometry/modify/symmetry" is_separator="true" position="4"/>
			<menu text="geometry/modify/rotate" action-command="_rotate" icon="edition-geometry-rotate" position="5"/>
			<menu text="geometry/modify/scale" action-command="_scale" icon="edition-geometry-scale" position="6"/>
			<menu text="geometry/modify/symmetry" action-command="_symmetry" icon="edition-geometry-symmetry" position="8"/>
			<menu text="geometry/modify/stretch" is_separator="true" position="9"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-copy" action-command="_copy" enable-text="copy" tooltip="copy" position="1"/>
				<selectable-tool icon="edition-geometry-rotate" action-command="_rotate" enable-text="rotate" tooltip="rotate" position="3"/>
				<selectable-tool icon="edition-geometry-scale" action-command="_scale" enable-text="scale" tooltip="scale" position="4"/>
				<selectable-tool icon="edition-geometry-symmetry" action-command="_symmetry" enable-text="symmetry" tooltip="symmetry" position="6"/>
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.MoveGeometryExtension"
			description="Extensión encargada de mover las geometrías."
			active="true">
			<menu text="geometry/modify/move" action-command="_move" icon="edition-geometry-move" position="2"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-move" action-command="_move" enable-text="move" tooltip="move" position="2"/>
			</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.StretchExtension"
			description="Extensión encargada de estirar geometrías en una capa en edición."
			active="true">
			<menu text="geometry/modify/stretch" action-command="_stretch" icon="edition-geometry-stretch" position="7"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-stretch" action-command="_stretch" enable-text="stretch" tooltip="stretch" position="5"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.JoinExtension"
			description="Extensión encargada de unir varias geometría en edición."
			active="true">
			<menu text="geometry/modify/join" action-command="_join" icon="edition-geometry-Join"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-Join" action-command="_join" enable-text="join" tooltip="join" position="8"/>
			</tool-bar>
		</extension>
 		<extension class-name="com.iver.cit.gvsig.SplitGeometryCADToolExtension"
			description="herramienta para dividir una geometria con una linea"
			active="true">
			<menu text="geometry/modify/Split" action-command="_split_geometry" icon="split-geometry"/>
			<menu text="geometry/modify/Split" is_separator="true"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="split-geometry"
							 action-command="_split_geometry"
							 tooltip="split_geometry"
							 enable-text="select_geometry_to_split"
							 last="false"
							 position="9"/>
			</tool-bar>
		 </extension>

		<extension class-name="com.iver.cit.gvsig.ExploitExtension"
			description="Extensión encargada de simplificar geometrías complejas."
			active="true">
			<menu text="geometry/modify/exploit" action-command="_exploit" icon="edition-geometry-exploit"/>
			<tool-bar name="cad_modificar" position="32">
				<action-tool icon="edition-geometry-exploit" action-command="_exploit" enable-text="exploit" tooltip="exploit" position="10"/>
			</tool-bar>
		</extension>

		<!--extension class-name="com.iver.cit.gvsig.BreakExtension"
			description="Extensión encargada de partir una geomtría en edición."
			active="true">
			<menu text="geometry/modify/break" action-command="_break" icon="edition-geometry-break"/>
			<tool-bar name="modificar" position="31">
				<selectable-tool icon="edition-geometry-break" action-command="_break" tooltip="break" position="9"/>
			</tool-bar>
		</extension-->

		<extension class-name="com.iver.cit.gvsig.MatrixExtension"
			description="Extensión encargada de crear una matriz a partir de la geometría seleccionada."
			active="true">
			<menu text="geometry/modify/matrix" action-command="_matrix" icon="edition-geometry-matrix"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-geometry-matrix" action-command="_matrix" tooltip="matrix" position="11"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.InternalPolygonExtension"
			description="Extensión encargada de insertar poligonos internos en una capa en edición."
			active="true">
			<menu text="geometry/modify/internal_polygon" is_separator="true"/>
			<menu text="geometry/modify/internal_polygon" action-command="_internalpolygon" icon="edition-modify-geometry-internalpolygon"/>
			<tool-bar name="cad_modificar" position="32">
				<selectable-tool icon="edition-modify-geometry-internalpolygon" action-command="_internalpolygon" enable-text="internal_polygon" tooltip="internal_polygon" position="12"/>
			</tool-bar>
		</extension>

		<!--extension class-name="com.iver.cit.gvsig.EquidistanceExtension"
			description="Extensión encargada de crear una geometría equidistante a otra."
			active="true">
			<menu text="geometry/modify/equidistance" action-command="_equidistance" icon="edition-geometry-equidistance"/>
			<tool-bar name="modificar" position="31">
				<selectable-tool icon="edition-geometry-equidistance" action-command="_equidistance" tooltip="equidistance" position="18"/>
			</tool-bar>
		</extension-->

		<extension class-name="com.iver.cit.gvsig.SelectionGeometryExtension"
			description="Extensión encargada de seleccionar las geometrías."
			active="true">
			<menu text="geometry/seleccionar" is_separator="true" position="50"/>
			<menu text="geometry/seleccionar" action-command="_selection" icon="edition-geometry-select" position="51"/>
			<tool-bar name="cad_selection" position="31">
				<selectable-tool icon="edition-geometry-select" action-command="_selection" tooltip="selection" position="1"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.ComplexSelectionGeometryExtension"
			description="Extensión encargada de la selección compleja de geometrías."
			active="true">
			<menu text="geometry/seleccion_compleja" action-command="_complex_selection" icon="edition-complex-selection" position="52"/>
			<tool-bar name="cad_selection" position="31">
				<selectable-tool icon="edition-complex-selection" action-command="_complex_selection" tooltip="complex_selection" position="2"/>
			</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.PreviousSelectionExtension"
			description="Extensión encargada de restablecer la selección anterior."
			active="true">
			<menu text="geometry/previous_selection" action-command="previous_selection" icon="previous-selection" position="53"/>
			<tool-bar name="cad_selection" position="31">
				<action-tool icon="previous-selection" action-command="previous_selection" enable-text="previous_selection" tooltip="previous_selection" position="3"/>
			</tool-bar>
		</extension>


		<extension class-name="com.iver.cit.gvsig.ViewCommandStackExtension"
			description="Extensión encargada de administrar la pila de comados de edición sobre una vista."
			active="true">
			<menu text="geometry/commands_stack" is_separator="true"/>
			<menu text="geometry/commands_stack" key="k" action-command="COMMANDSTACK" icon="commands-stack"/>
		<tool-bar name="undoredo" position="9">
			<action-tool name="commands_stack" icon="commands-stack" action-command="COMMANDSTACK" tooltip="commands_stack" position="3"/>
		</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.TableCommandStackExtension"
			description="Extensión encargada de administrar la pila de comados de edición sobre una tabla."
			active="true">
			<menu text="Tabla/commands_stack" action-command="COMMANDSTACK" icon="commands-stack" last="true"/>
		<tool-bar name="undoredo" position="9">
			<action-tool name="commands_stack" icon="commands-stack" action-command="COMMANDSTACK" tooltip="commands_stack" position="3"/>
		</tool-bar>
		</extension>

		<extension class-name="com.iver.cit.gvsig.UndoViewExtension"
			description="Extensión encargada de dehacer la edición sobre la capa activa."
			active="true" >
			<menu text="geometry/undo" is_separator="true" position="1000"/>
			<menu text="geometry/undo" key="z" action-command="UNDO" icon="view-undo" position="1001"/>
		<tool-bar name="undoredo" position="9">
				<action-tool name="deshacer" icon="view-undo" action-command="UNDO" enable-text="undo" tooltip="undo" position="1"/>
		</tool-bar>
		</extension>
		<extension class-name="com.iver.cit.gvsig.RedoViewExtension"
			description="Extensión encargada de rehacer la edición sobre la capa activa."
			active="true">
			<menu text="geometry/redo" key="r" action-command="REDO" icon="view-redo" position="1002"/>
		<tool-bar name="undoredo" position="9">
				<action-tool name="redo" icon="view-redo" action-command="REDO" enable-text="redo" tooltip="redo" position="2"/>
		</tool-bar>
		</extension>

		<!--extension class-name="com.iver.cit.gvsig.LayerPropertiesExtension"
			description="Abre el diálogo de propiedades de la capa."
			active="true" >
			<menu text="Capa/Propiedades" action-command="PROPERTIES"/>
		</extension-->

	</extensions>
</plugin-config>
