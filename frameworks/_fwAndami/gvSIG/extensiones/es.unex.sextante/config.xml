<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
  <depends plugin-name="com.iver.cit.gvsig"/>
  <libraries library-dir="."/>
  <extensions>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteToolboxExtension"
	       description="SEXTANTE Toolbox"
	       active="true">
      <tool-bar name="SEXTANTE" position="2">
	<action-tool icon="images/sextante.gif" tooltip="SEXTANTE Toolbox" position="1"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteModelerExtension"
               description="Modeler"
               active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/model.png" tooltip="SEXTANTE Modeler" position="2"/>
      </tool-bar>
    </extension>

    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteHistoryExtension"
               description="History"
               active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/history.gif" tooltip="SEXTANTE History" position="4"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteCommandLineExtension"
               description="SEXTANTE command line"
               active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/terminal.png" tooltip="SEXTANTE command line" position="3"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteAdditionalResultsExtension"
               description="Results"
               active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/chart.gif" tooltip="Results" position="5"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteAboutExtension"
	       description="Data"
	       active="true">
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteDataExplorerExtension" description="Data" active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/documenter.png" tooltip="Data" position="7"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextantePointCollectorExtension" description="Catch coordinates" active="true">
      <tool-bar name="SEXTANTE" position="2">
        <action-tool icon="images/bullseye.gif" tooltip="Catch coordinates" position="6"/>
      </tool-bar>
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteAboutExtension" description="" active="true">
    </extension>
    <extension class-name="es.unex.sextante.gvsig.extensions.SextanteToolbarConfigExtension" description="SEXTANTE Toolbar" active="true">
      <tool-bar name="SEXTANTEToolbar" position="3">
        <action-tool icon="images/sextante2.gif" tooltip="SEXTANTE Toolbar" position="1"/>
      </tool-bar>
    </extension>
  </extensions>
</plugin-config>
